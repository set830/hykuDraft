<div class="page-header">
  <h1>
    Outputs
    <a href="/outputs/new" class="btn btn-lg btn-success">New Output</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Actions</th>
      </tr>

      <% @outputs.each do |output| %>
      <tr>
        <td>
          <a href="/outputs/<%= output.id %>" class="btn btn-primary">Show</a>
          <a href="/outputs/<%= output.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_output/<%= output.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
