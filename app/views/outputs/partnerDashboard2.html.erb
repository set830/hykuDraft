<html>
  <head>
  <link rel="stylesheet" href="/stylesheets/style.css">
    <script src="js/wow.mis.js"></script>
    <script>
      new WOW().init();
    </script>

    <!-- Override Navbar to add navbarImage, not DRY -->
    <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
      <div class="container">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a href="/" class="navbar-brand">Hyku</a>
        </div>

        <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">

          <ul class="nav navbar-nav navbar-right">
              <li><a href="/users/sign_in">Partner with Hyku</a></li>
              <li><a href="/users/sign_up">Log In</a></li>
          </ul>
        </div><!-- /.navbar-collapse -->
      </div>
    </nav>

  </head>

<body>

<div class="container-fluid">
  <div class="row">
    <div class="col-sm-offset-1 col-sm-10 vcenter">
    </div>
  </div>
</div>



<div class="jumbotron lightgray">
  <br />
  <br />
  <br />
  <div id="personalized" class="container white">
    <div class="container">
      <br />
      <br />
      <ul class="nav nav-pills nav-justified">
        <li role="presentation" class="active inactive"><a href="#newPill" aria-controls="Create a New Offer" role="tab" data-toggle="pill">Create a New Offer</a></li>
        <li role="presentation" class="inactive"><a href="#offerPill" aria-controls="Offers" role="tab" data-toggle="pill">Offers</a></li>
        <li role="presentation" class="inactive"><a href="#editPill" aria-controls="Edit Profile" role="tab" data-toggle="pill">Edit Profile</a></li>
        <li role="presentation" class="inactive"><a href="#bankPill" aria-controls="Bank Account" role="tab" data-toggle="pill">Bank Account</a></li>
      </ul>

      <div class="tab-content">
        <div id="newPill" class="tab-pane fade in active">
          <br />
          <br />
          <button type="button" class="btn btn-default">Reload</button>
          <br />
          <small id="refreshHelp" class="form-text text-muted">with previously submitted data</small>
          <br />
          <form>
            <h3 class="blue">
              Event Details
            </h3>
            <hr class="blueline">
            <div class="form-group">
              <label for="eventTitle"><h4 class="darkergray">Event Title</h4></label>
              <input type="text" class="form-control" id="eventTitle" aria-describedby="eventTitle" placeholder="Provide a short distinct name">
            </div>
            <div class="row">
              <div class="col-sm-4">
                <div class="form-group">
                  <label for="eventDate"><h4 class="darkergray">Event Date</h4></label>
                  <input type="date" class="form-control" id="eventDate" aria-describedby="eventDate" placeholder="May 12, 2017">
                </div>
              </div>
              <div class="col-sm-4">
                <div class="form-group">
                  <label for="eventStartTime"><h4 class="darkergray">Start Time</h4></label>
                  <input type="time" class="form-control" id="eventStartTime" aria-describedby="eventStartTime" placeholder="3:00pm">
                </div>
              </div>
              <div class="col-sm-4">
                <div class="form-group">
                  <label for="eventEndTime"><h4 class="darkergray">End Time</h4></label>
                  <input type="time" class="form-control" id="eventEndTime" aria-describedby="eventEndTime" placeholder="5:00pm">
                </div>
              </div>
            </div>
            <br />
            <h4 class="darkergray">Location</h4>
            <div class="form-group">
              <input type="text" class="form-control" id="eventVenue" aria-describedby="eventVenue" placeholder="Enter the Venue's Name">
              <input type="text" class="form-control" id="eventStreetAddress" aria-describedby="eventStreetAddress" placeholder="Street Address">
              <input type="text" class="form-control" id="eventStreetAddress2" aria-describedby="eventStreetAddress2" placeholder="Street Address 2">
              <input type="text" class="form-control" id="eventNeighborhood" aria-describedby="eventNeighborhood" placeholder="Neighborhood">
              <input type="text" class="form-control" id="eventCity" aria-describedby="eventCity" placeholder="City">
              <div class="row">
                <div class="col-sm-6">
                  <input type="text" class="form-control" id="eventState" aria-describedby="eventState" placeholder="State">
                </div>
                <div class="col-sm-6">
                  <input type="text" class="form-control" id="eventZip" aria-describedby="eventZip" placeholder="Zip Code">
                </div>
              </div>
            </div>
            <br />
            <h4 class="darkergray">Cover Photo</h4>
            <div class="form-group">
              <input type="file" class="form-control-file" id="coverPhoto" aria-describedby="coverPhoto">
              <small id="fileHelp" class="form-text text-muted">JPEG or PNG at least 400 pixels wide and 150 pixels tall</small>
            </div>
            <br />
            <h3 class="blue">
              Pricing Details
            </h3>
            <hr class="blueline">

            <div class="form-group">
              <label for="individualPrice"><h4 class="darkergray">Individual Price</h4></label>
              <div class="row">
                <div class="col-sm-6">
                  <input type="text" class="form-control" id="retailPrice" placeholder="Retail Price">
                </div>
                <div class="col-sm-6">
                  <input type="text" class="form-control" id="offerPrice" aria-describedby="offerPrice" placeholder="Offer Price">
                </div>
              </div>
            </div>

            <div class="form-group">
              <label for="groupPrice"><h4 class="darkergray">Group Price</h4></label>
              <div class="row">
                <div class="col-sm-6">
                  <input type="text" class="form-control" id="groupSize" placeholder="Group Size">
                </div>
                <div class="col-sm-6">
                  <input type="text" class="form-control" id="groupPrice" aria-describedby="groupPrice" placeholder="Discounted Per Person Price">
                </div>
              </div>
              <small id="neighborhoodHelp" class="form-text text-muted">For a group of this size, we will charge each person this price. <br />For groups less than this number, we will dynamically calculate the price based on the offer price and this discounted price. <br />For groups greater than this number, we will continue to charge this discounted price.</small>
            </div>

            <div class="form-group">
              <label for="eventPerks"><h4 class="darkergray">Perks</h4></label>
              <div class="row">
                <div class="col-sm-6">
                  <input type="text" class="form-control" id="groupSize" placeholder="For this Group Size">
                </div>
                <div class="col-sm-6">
                  <input type="text" class="form-control" id="groupPrice" aria-describedby="groupPrice" placeholder="Description of Perk">
                </div>
              </div>
              <div class="row">
                <div class="col-sm-6">
                  <input type="text" class="form-control" id="groupSize" placeholder="For this Group Size">
                </div>
                <div class="col-sm-6">
                  <input type="text" class="form-control" id="groupPrice" aria-describedby="groupPrice" placeholder="Description of Perk">
                </div>
              </div>
              <div class="row">
                <div class="col-sm-6">
                  <input type="text" class="form-control" id="groupSize" placeholder="For this Group Size">
                </div>
                <div class="col-sm-6">
                  <input type="text" class="form-control" id="groupPrice" aria-describedby="groupPrice" placeholder="Description of Perk">
                </div>
              </div>
              <small id="neighborhoodHelp" class="form-text text-muted">For future version. Comment out this code for now.</small>
            </div>

            <div class="form-group">
              <label for="eventTitle"><h4 class="darkergray">Restrictions</h4></label>
              <div class="row">
                <div class="col-sm-6">
                  <input type="text" class="form-control" id="groupMax" placeholder="Max Group Size">
                </div>
                <div class="col-sm-6">
                </div>
              </div>
              <small id="neighborhoodHelp" class="form-text text-muted">Size of group restricted to this number. If unlimited number can join group, leave blank.</small>
            </div>
            <div class="form-group">
                <input type="text" class="form-control" id="groupSize" placeholder="Fine Print Description Bullet Point 1">
                <input type="text" class="form-control" id="groupPrice" aria-describedby="groupPrice" placeholder="Fine Print Description Bullet Point 2">
                <input type="text" class="form-control" id="groupPrice" aria-describedby="groupPrice" placeholder="Fine Print Description Bullet Point 3">
                <small id="neighborhoodHelp" class="form-text text-muted">Use this to communicate any important information (e.g. refunds, exchanges, etc.)</small>
            </div>

            <br />
            <h3 class="blue">
              Company Details
            </h3>
            <hr class="blueline">
            <div class="form-group">
              <label for="Partner Description"><h4 class="darkergray">Description</h4></label>
              <div class="row">
                <div class="col-sm-6">
                  <input type="text" class="form-control" id="companyDisplayName" aria-describedby="companyDisplayName" placeholder="Company Name">
                </div>
                <div class="col-sm-6">
                </div>
              </div>
              <div class="row">
                <div class="col-sm-12">
                  <textarea class="form-control" id="exampleTextarea" rows="3" placeholder="Provide a short company description"></textarea>
                </div>
              </div>
            </div>

            <div class="form-group">
              <label for="businessContact"><h4 class="darkergray">Contact</h4></label>
              <div class="row">
                <div class="col-sm-6">
                  <input type="text" class="form-control" id="contactPhoneNumber" aria-describedby="contactPhoneNumber" placeholder="Phone Number">
                </div>
                <div class="col-sm-6">
                  <input type="email" class="form-control" id="contactEmail" aria-describedby="contactEmail" placeholder="E-mail">
                </div>
              </div>
            </div>
            <br />
            <button type="submit" class="btn btn-success btn-large">Submit</button>
          </form>
          <br />
          <br />
        </div>

        <div id="offerPill" class="tab-pane fade">
          <br />
          <br />
            <div class="container">
              <div class="panel panel-default dashboardPanel">
                <div class="panel-body">
                  <div class="container">
                    <div class="row">
                      <div class="col-xs-12">
                        <h4 class="blue">
                          Created Offers
                        </h4>
                        <br />
                        <div class="scrollit">
                          <table class="table table-bordered table-striped table-hover table-responsive centeralign">
                            <col width="25%">
                            <col width="25%">
                            <col width="25%">
                            <col width="25%">
                              <tr>
                                <th class="centeralign">Creation Date</th>
                                <th class="centeralign">Event Date</th>
                                <th class="centeralign">Product Name</th>
                                <th class="centeralign">Hyku Page</th>
                              </tr>
                              <tr class="darkergray">
                                <td>"CreatedDate"</td>
                                <td>"EventDate"</td>
                                <td>"ProductName"</td>
                                <td class="darkergray centeralign"><a href="">View</a></td>
                              </tr>
                              <tr class="darkergray">
                                <td>"CreatedDate"</td>
                                <td>"EventDate"</td>
                                <td>"ProductName"</td>
                                <td class="darkergray centeralign"><a href="">View</a></td>
                              </tr>
                              <tr class="darkergray">
                                <td>"CreatedDate"</td>
                                <td>"EventDate"</td>
                                <td>"ProductName"</td>
                                <td class="darkergray centeralign"><a href="">View</a></td>
                              </tr>
                          </table>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          <br />
          <br />
            <div class="container">
              <div class="panel panel-default dashboardPanel">
                <div class="panel-body">
                  <div class="container">
                    <div class="row">
                      <div class="col-xs-12">
                        <h4 class="blue">
                          Activated Hykus
                        </h4>
                        <br />
                        <div class="scrollit">
                          <table class="table table-bordered table-striped table-hover table-responsive centeralign">
                            <col width="16.6%">
                            <col width="16.6%">
                            <col width="16.6%">
                            <col width="16.6%">
                            <col width="16.6%">
                            <col width="16.6%">
                              <tr>
                                <th class="centeralign">Event Date</th>
                                <th class="centeralign">Product Name</th>
                                <th class="centeralign">Time Left</th>
                                <th class="centeralign">Number of Joiners</th>
                                <th class="centeralign">Organizer Email</th>
                                <th class="centeralign">Hyku Page</th>
                              </tr>
                              <tr class="darkergray">
                                <td>"EventDate"</td>
                                <td>"ProductName"</td>
                                <td>"TimeLeft"</td>
                                <td>"NumberOfJoiners"</td>
                                <td>"OrganizerEmail"</td>
                                <td class="darkergray centeralign"><a href="">View</a></td>
                              </tr>
                              <tr class="darkergray">
                                <td>"EventDate"</td>
                                <td>"ProductName"</td>
                                <td>"TimeLeft"</td>
                                <td>"NumberOfJoiners"</td>
                                <td>"OrganizerEmail"</td>
                                <td class="darkergray centeralign"><a href="">View</a></td>
                              </tr>
                              <tr class="darkergray">
                                <td>"EventDate"</td>
                                <td>"ProductName"</td>
                                <td>"TimeLeft"</td>
                                <td>"NumberOfJoiners"</td>
                                <td>"OrganizerEmail"</td>
                                <td class="darkergray centeralign"><a href="">View</a></td>
                              </tr>
                          </table>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
        </div>
        <div id="editPill" class="tab-pane fade">
          <br />
          <br />
          <form>
            <h3 class="blue">
              Update Contact Information
            </h3>
            <hr class="blueline">
            <div class="form-group">
              <label for="partnerName"><h4 class="darkergray">Business Name</h4></label>
              <input type="text" class="form-control" id="partnerName" aria-describedby="partnerName" placeholder="">
            </div>
            <div class="row">
              <div class="col-sm-6">
                <div class="form-group">
                  <label for="ownerFirstName"><h4 class="darkergray">Owner Name</h4></label>
                  <input type="date" class="form-control" id="ownerFirstName" aria-describedby="ownerFirstName" placeholder="First Name">
                </div>
              </div>
              <div class="col-sm-6">
                <div class="form-group">
                  <label for="ownerLastName"><h4 class="whiteC">.</h4></label>
                  <input type="time" class="form-control" id="ownerLastName" aria-describedby="ownerLastName" placeholder="Last Name">
                </div>
              </div>
            </div>
            <div class="row">
              <div class="col-sm-6">
                <div class="form-group">
                  <label for="ownerPhoneNumber"><h4 class="darkergray">Contact</h4></label>
                  <input type="date" class="form-control" id="ownerPhoneNumber" aria-describedby="ownerPhoneNumber" placeholder="Phone Number">
                </div>
              </div>
              <div class="col-sm-6">
                <div class="form-group">
                  <label for="ownerEmail"><h4 class="whiteC">.</h4></label>
                  <input type="time" class="form-control" id="ownerEmail" aria-describedby="ownerEmail" placeholder="E-mail">
                </div>
              </div>
            </div>
            <br />
            <button type="submit" class="btn btn-success btn-large">Submit</button>
          </form>
          <br />
          <br />
        </div>
        <div id="bankPill" class="tab-pane fade">
          <div class="row">
            <div class="col-sm-4">
            </div>
            <div class="col-sm-4">
              <br />
              <br />
              <br />
              <div class="panel panel-default">
                <div class="panel-body centeralign connectedPadding">
                  <h5 class="big">
                    <span class="green">CONNECTED</span> or <span class="red">NOT CONNECTED</span>
                  </h5>
                  <br />
                    <button type="button" class="btn btn-info">Connect with Stripe</button>
                  <br />
                  <br />
                </div>
              </div>
            </div>
            <div class="col-sm-4">
            </div>
          </div>
          <br />
          <br />
          <br />
        </div>
      </div>
    </div>
  </div>
</div>

<%= render 'footer' %>

</body>

</html>

